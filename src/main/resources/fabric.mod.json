{
	"schemaVersion": 1,
	"id": "myfabricmod",
	"version": "${version}",
	"name": "MyFabricMod",
	"description": "My first fabric mod!",
	"authors": [
		"CrazyheadJake"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/myfabricmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.crazyheadjake.myfabricmod.MyFabricMod"
		],
		"client": [
			"net.crazyheadjake.myfabricmod.MyFabricModClient"
		],
		"fabric-datagen": [
			"net.crazyheadjake.myfabricmod.MyFabricModDataGenerator"
		]
	},
	"mixins": [
		"myfabricmod.mixins.json",
		{
			"config": "myfabricmod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}